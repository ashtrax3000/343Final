      </Label>
      <Label layoutX="141.0" layoutY="78.0" text="Customer" />
      <TextField fx:id="customerName" editable="false" layoutX="269.0" layoutY="74.0" />
      <Label layoutX="154.0" layoutY="134.0" text="Date" />
      <DatePicker fx:id="date" layoutX="257.0" layoutY="130.0" />
      <Label layoutX="99.0" layoutY="248.0" text="Contact" />
      <Label layoutX="153.0" layoutY="192.0" text="Time" />
      <ComboBox fx:id="time" layoutX="269.0" layoutY="188.0" prefWidth="150.0" />
      <ComboBox fx:id="contact" layoutX="153.0" layoutY="244.0" onAction="#handleLocation" prefWidth="150.0" />
      <Label layoutX="116.0" layoutY="314.0" text="Appointment Type" />
      <ComboBox fx:id="type" layoutX="237.0" layoutY="310.0" prefHeight="25.0" prefWidth="213.0" />
      <Label layoutX="344.0" layoutY="248.0" text="Location" />
      <TextField fx:id="location" editable="false" layoutX="396.0" layoutY="244.0" />
